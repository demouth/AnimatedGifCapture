{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "version": "1.0.1",
  "description": "__MSG_extDescription__",
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icon/icon.png"
  },
  "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": [
        "content/init.js"
      ]
  }],
  "options_page": "options/options.html",
  "permissions": [
    "storage",
    "http://*/",
    "<all_urls>",
    "desktopCapture",
    "tabCapture"
  ],
   "icons": {
      "16": "icon/icon_16.png",
      "19": "icon/icon.png",
      "32": "icon/icon_32.png",
      "48": "icon/icon_48.png",
      "128": "icon/icon_128.png"
   },
  "background": {
    "page": "background/init.html"
  }
}